
        <h1><i class="fa-solid fa-cloud-moon"></i>Contact List</h1>
        <button id="add-button" class="btn btn-primary btn-sm mb-1">
            <i class="fas fa-plus-circle fa-sm">
                Add contact
            </i>
        </button>
        <div class="row">
            <div class="col">
                <table id="contact-list" class="table table-striped table-boarded table-hover">
                    <thead class="table-dark">
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">Full Name</th>
                        <th scope="col">Contact Number</th>
                        <th scope="col">Email Address</th>
                        <th scope="col">Edit Contact</th>
                        <th scope="col">Delete Contact</th>
                    </tr>
                    </thead>
                    <tbody id="contactList">
                    <% for (let count=0; count < contacts.length; count++) { %>
                        <tr>
                            <th scope="row" class="text-center"><%= count + 1 %></th>
                            <td><%= contacts[count].FullName %></td>
                            <td><%= contacts[count].ContactNumber %></td>
                            <td><%= contacts[count].EmailAddress %></td>

                            <td class="text-center">
                                <a href="/edit/<%= contacts[count]._id %>" class="btn btn-primary btn-sm edit">
                                    <i class="fas fa-edit fa-sm"> Edit</i>
                                </a>
                            </td>
                            <td class="text-center">
                                <a href="/delete/<%= contacts[count]._id %>" class="btn btn-danger btn-sm delete">
                                    <i class="fas fa-edit fa-sm"> Delete</i>
                                </a>
                            </td>
                        </tr>
                    <% } %>
                    </tbody>
                </table>
            </div>
        </div>